{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "types": ["dotenv"],
      "target": "ES6",
      "module": "commonjs",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true
   },
      "include" : [
         "src/**/*.ts",
         "config/**/*.ts"
      ],
      "exclude": [
         "node_modules"
      ],
}